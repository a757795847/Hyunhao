<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.PayCredential" table="pay_credential" schema="jt_platform">
        <id name="appid" column="appid"/>
        <property name="mchid" column="mchid"/>
        <property name="key" column="key"/>
        <property name="wishing" column="wishing"/>
        <property name="sendName" column="send_name"/>
        <property name="credentialPath" column="credential_path"/>
        <property name="clientIp" column="client_ip"/>
        <property name="actName" column="act_name"/>
        <property name="remark" column="remark"/>
    </class>
</hibernate-mapping>