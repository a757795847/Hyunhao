<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.TokenConfig" table="token_config" schema="jt_platform">
        <id name="name" column="name"/>
        <property name="token" column="token"/>
        <property name="expiresIn" column="expires_in"/>
        <property update="false" insert="false" name="insertTime" column="insert_time"/>
        <property update="false" insert="false" name="updateTime" column="update_time"/>
    </class>
</hibernate-mapping>