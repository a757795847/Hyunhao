<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.ValidData" table="valid_data" schema="jt_platform">
        <id name="id" column="id"/>
        <property name="username" column="username"/>
        <property name="tappid" column="tappid"/>
        <property insert="false" update="false" name="insertime" column="insertime"/>
        <property name="endData" column="end_data"/>
        <property name="beginTime" column="begin_time"/>
        <property name="zyappid" column="zyappid"/>
    </class>
</hibernate-mapping>