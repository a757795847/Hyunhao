<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.GeCode" table="ge_code" schema="jt_platform">
        <id name="geCodeM" column="ge_codeM"/>
        <property name="geAppid" column="ge_appid"/>
        <property update="false" insert="false" name="insertTime" column="insert_time"/>
        <property update="false" insert="false" name="updateTime" column="update_time"/>
        <property name="expires" column="expires"/>
        <property name="callbackUrl" column="callback_url"/>
        <property name="openid" column="openid"/>
        <property name="wxToken" column="wx_token"/>
        <property name="state" column="state"/>
