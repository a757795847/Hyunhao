<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.WxToken" table="wx_token" schema="jt_platform">
        <id name="userOpenId" column="user_open_id"/>
        <property name="wxToken" column="wx_token"/>
        <property insert="false" name="insertTime" column="insert_time"/>
        <property insert="false" name="updateTime" column="update_time"/>
        <property name="expires" column="expires"/>
        <property name="refreshToken" column="refresh_token"></property>
        <property name="scope" column="scope"/>
    </class>
</hibernate-mapping>