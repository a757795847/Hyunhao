<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.DataStrategy" table="data_strategy" schema="jt_platform">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="userId" column="user_id"/>
        <property name="name" column="name"/>
        <property name="money" column="money"/>
        <property name="remark" column="remark"/>
        <property name="createUserId" column="create_user_id"/>
        <property name="createDate" column="create_date"/>
        <property name="updateUserId" column="update_user_id"/>
        <property name="updateDate" column="update_date"/>
        <property name="delFlag" column="del_flag"/>
    </class>
</hibernate-mapping>