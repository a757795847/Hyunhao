<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.DataRecharge" table="data_recharge" schema="jt_platform">
        <id name="id" column="id">
        <generator class="native"/>
        </id>
        <property name="weixinAppid" column="weixin_appid"/>
        <property name="mobile" column="mobile"/>
        <property name="weixinNumber" column="weixin_number"/>
        <property name="money" column="money"/>
        <property name="days" column="days"/>
        <property name="state" column="state"/>
        <property name="kind" column="kind"/>
        <property name="startDate" column="start_date"/>
        <property name="endDate" column="end_date"/>
        <property name="createUserId" column="create_user_id"/>
        <property name="createDate" column="create_date"/>
        <property name="updateUserId" column="update_user_id"/>
        <property name="updateDate" column="update_date"/>
        <property name="delFlag" column="del_flag"/>
    </class>
</hibernate-mapping>