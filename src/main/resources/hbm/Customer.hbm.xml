<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.Customer" table="customer" schema="jt_platform">
        <id name="customerId" column="customer_id">
            <generator class="native"></generator>
        </id>
        <property name="name" column="name"/>
        <property name="phone" column="phone"/>
        <property update="false" insert="false" name="insertTime" column="insert_time"/>
        <property update="false" insert="false" name="updateTime" column="update_time"/>
    </class>
</hibernate-mapping>