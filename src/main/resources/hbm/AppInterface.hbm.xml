<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.AppInterface" table="app_interface" schema="jt_platform">
        <id name="geAppid" column="ge_appid"/>
        <property name="wxAppid" column="wx_appid"/>
        <property name="secret" column="secret"/>
        <property name="customerId" column="customer_id"/>
        <property name="scope" column="scope"/>
        <property insert="false" name="insertTime" column="insert_time"/>
        <property insert="false" name="updateTime" column="update_time"/>
    </class>
</hibernate-mapping>