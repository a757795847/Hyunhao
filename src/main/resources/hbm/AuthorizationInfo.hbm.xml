<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.AuthorizationInfo" table="authorization_info" schema="jt_platform">
        <id name="authorizerAppid" column="authorizer_appid"/>
        <property name="authorizerAccessToken" column="authorizer_access_token"/>
        <property name="expiresIn" column="expires_in"/>
        <property name="authorizerRefreshToken" column="authorizer_refresh_token"/>
        <property name="funcInfo" column="func_info"/>
        <property name="insertTime" column="insert_time"/>
        <property name="updateTime" column="update_time"/>
    </class>
</hibernate-mapping>