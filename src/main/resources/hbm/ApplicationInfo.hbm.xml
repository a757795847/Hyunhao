<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.ApplicationInfo" table="application_info" schema="jt_platform">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="price" column="price"/>
        <property name="applicationInfo" column="application_info"/>
        <property name="backgroundColor" column="background_color"/>
        <property name="deleteFlag" column="delete_flag"/>
        <property name="description" column="description"/>
        <property name="escapeClause" column="escape_clause"/>
        <property name="imageUrl" column="image_url"/>
    </class>
</hibernate-mapping>