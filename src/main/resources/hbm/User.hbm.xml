<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.User" table="user" schema="jt_platform">
        <id name="username" column="username"/>
        <property name="password" column="password"/>
        <property insert="false" update="false" name="insertTime" column="insert_time"/>
        <property name="name" column="name"/>
        <property name="endTime" column="end_time"/>
        <property insert="false" update="false" name="updateTime" column="update_time"/>
        <property name="role" column="role"/>
        <property name="isEnable" column="is_enable"/>
        <property name="isAuthentication" column="is_authentication"/>
        <property name="phone" column="phone"/>
        <property name="authenticationTime" column="authentication_time"/>
        <property name="state" column="state"/>
    </class>
</hibernate-mapping>