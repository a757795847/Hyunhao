<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.RedStatus" table="red_status" schema="jt_platform">
        <id name="mchBillno" column="mch_billno"/>
        <property name="mchId" column="mch_id"/>
        <property name="status" column="status"/>
        <property name="totalAmount" column="total_amount"/>
        <property name="sendTime" column="send_time"/>
        <property name="refundTime" column="refund_time"/>
        <property name="rcvTime" column="rcv_time"/>
        <property name="openid" column="openid"/>
        <property update="false" insert="false" name="insertTime" column="insert_time"/>
        <property update="false" insert="false" name="updateTime" column="update_time"/>
        <property name="wxappid" column="wxappid"/>
        <property name="tappid" column="tappid"/>
    </class>
</hibernate-mapping>