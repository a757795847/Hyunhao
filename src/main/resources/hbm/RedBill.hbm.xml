<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.gcode.pojo.RedBill" table="red_bill" schema="jt_platform">
        <id name="mchBillno" column="mch_billno"/>
        <property name="mchId" column="mch_id"/>
        <property name="wxappid" column="wxappid"/>
        <property name="reOpenid" column="re_openid"/>
        <property name="totalAmount" column="total_amount"/>
        <property name="sendListid" column="send_listid"/>
        <property name="sign" column="sign"/>
        <property name="status" column="status"/>
        <property name="tappid" column="tappid"/>
        <property update="false" insert="false" name="insertTime" column="insert_time"/>
        <property update="false" insert="false" name="updateTime" column="update_time"/>
    </class>
</hibernate-mapping>